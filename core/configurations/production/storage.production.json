{
	"provider": "s3",
	"bucket": "${S3_BUCKET}",
	"region": "${S3_REGION}",
	"accessKeyId": "${S3_ACCESS_KEY}",
	"secretAccessKey": "${S3_SECRET_KEY}",
	"cdnUrl": "${CDN_URL}",
	"maxFileSizeMb": 10,
	"allowedMimeTypes": ["image/png", "image/jpeg", "image/webp", "application/pdf"],
	"avatarMaxSizeKb": 512,
	"bannerMaxSizeKb": 2048
}
