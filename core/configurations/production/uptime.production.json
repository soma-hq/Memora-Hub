{
	"enabled": true,
	"intervalSeconds": 30,
	"endpoints": [
		{ "name": "API Health", "url": "${APP_BASE_URL}/api/health" },
		{ "name": "Database", "url": "${APP_BASE_URL}/api/health/db" }
